{
  "rewrites": [
    {
      "source": "/favicon.ico",
      "destination": "/public/favicon.svg"
    }
  ],
  "headers": [
    {
      "source": "/(css|js|assets|public)/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}